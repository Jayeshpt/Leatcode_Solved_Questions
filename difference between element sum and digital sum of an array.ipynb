   "source": [
    "class Solution:\n",
    "    def differenceOfSum(self, nums: List[int]) -> int:\n",
    "        sum1 = sum(nums)\n",
    "        sum2 = 0\n",
    "        for i in nums:\n",
    "            for j in str(i):\n",
    "                sum2 += int(j)\n",
    "        return abs(sum1 - sum2)\n",
    "            \n",
    "            "
   ]
  }
