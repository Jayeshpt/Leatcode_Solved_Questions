   "source": [
    "class Solution:\n",
    "    def maximumCount(self, nums: List[int]) -> int:\n",
    "        neg = 0\n",
    "        pos = 0\n",
    "        for i in nums:\n",
    "            if i < 0 :\n",
    "                pos +=1\n",
    "            if i > 0:\n",
    "                neg +=1\n",
    "        return max(pos,neg)"
   ]
  }
